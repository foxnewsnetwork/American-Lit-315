<!-- Header file -->
<%= yield :head %>

<header>
<nav>
<div class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <a class="brand" href="/">Dashboard</a>
      <ul class="nav">
		<% if user_signed_in? %>
			<li class=""><%= link_to "My Dashboard", current_user %><li>
			<li class=""><%= link_to "Logout", destroy_user_session_path, :method => :delete %></li>
		<% else %>
			<li class=""><%= link_to "Gamers", users_path %></li>
		<% end %>
		<% if company_signed_in? %>
			<li class=""><%= link_to "My Dashboard", current_company %><li>
			<li class=""><%= link_to "Logout", destroy_company_session_path, :method => :delete %></li>
		<% else %>
			<li class=""><%= link_to "Brand Partners", companies_path %></li>
		<% end %>
		<% if publisher_signed_in? %>
			<li class=""><%= link_to "My Dashboard", current_publisher %><li>
			<li class=""><%= link_to "Logout", destroy_publisher_session_path, :method => :delete %></li>
		<% else %>
			<li class=""><%= link_to "Platform Partners", publishers_path %></li>
		<% end %>
	</ul>
    </div>
  </div>
</div>
</header>
</nav>
<br />
<br />
<br />

